
{
	"manifest_version": 2,
	"name": "__MSG_coordGPSName__",
	"version": "1.0.0",
	"description": "__MSG_coordGPSDescription__",
	"homepage_url": "https://github.com/Silbad/coordGPS",
	"default_locale": "en",
	"icons": {
